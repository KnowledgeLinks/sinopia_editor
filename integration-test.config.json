{
  "preset": "jest-puppeteer",
  "setupFilesAfterEnv": [
    "<rootDir>/setupEnzyme.js"
  ],
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "moduleNameMapper": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
    "\\.(css|less)$": "identity-obj-proxy"
  },
  "collectCoverageFrom": [
    "src/**/*.js",
    "src/**/*.jsx",
    "!src/index.js"
  ],
  "reporters": [
    "default",
    "jest-junit"
  ],
  "testPathIgnorePatterns": [
    "/node_modules/",
    "__tests__/fixtureLoaderHelper.js",
    "__tests__/integration/loginHelper",
    "__tests__/integration/previewRDFHelper",
    "__tests__/integration/integrationHelper"
  ],
  "setupFiles": [
    "jest-localstorage-mock",
    "jest-prop-type-error",
    "dotenv/config"
  ],
  "testMatch": ["**/__tests__/integration/**/*.[jt]s?(x)"]
}
