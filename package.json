{
  "name": "sinopia_editor",
  "description": "Editor for BIBFRAME data forked from http://github.com/lcnetdev/bfe",
  "keywords": [
    "bibframe",
    "editor",
    "rdf"
  ],
  "version": "0.0.1",
  "homepage": "http://github.com/LD4P/sinopia_editor/",
  "engines": {
    "node": "10.11.x"
  },
  "contributors": [
    "Kirk Hess <khes@loc.gov>",
    "Jeremy Nelson"
  ],
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "es6-plato": "^1.0.18",
    "eslint": "^5.7.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-takeoff": "0.0.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-junit": "^5.1.0",
    "jest-puppeteer": "^3.4.0",
    "jsdom": "13.0.0",
    "jsdom-global": "3.0.2",
    "puppeteer": "^1.8.0",
    "react-hot-loader": "^4.3.11",
    "style-loader": "^0.23.1",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "bugs": {
    "url": "http://github.com/LD4P/sinopia_editor/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "analysis": "es6-plato -d static-analysis -n -e .eslintrc.js src src/lib static/js/config.js",
    "build": "webpack -p --progress --mode production",
    "dev-build": "webpack --progress --mode development",
    "dev-build-test": "npm run dev-build && npm run test",
    "dev-start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "eslint": "eslint --max-warnings 600 --color -c .eslintrc.js --ext js,jsx ./src",
    "jest-cov": "jest --coverage --colors",
    "jest-ci": "jest --ci --runInBand --coverage --reporters=default --reporters=jest-junit --colors  && cat ./coverage/lcov.info | coveralls",
    "test": "jest --colors"
  },
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "babel-core": "^7.0.0-bridge.0",
    "express": "^4.16.3",
    "identity-obj-proxy": "^3.0.0",
    "merge": "^1.2.1",
    "react": "^16.6.0",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.6.0",
    "react-offcanvas": "^0.3.1",
    "react-router-dom": "^4.3.1",
    "x2js": "^3.2.1"
  },
  "jest": {
    "preset": "jest-puppeteer",
    "setupTestFrameworkScriptFile": "<rootDir>/setupEnzyme.js",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.js",
      "src/**/*.jsx",
      "!src/bfe*.js",
      "!src/lib/aceconfig.js",
      "!static/js/config*.js"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "test-config.js"
    ]
  }
}
