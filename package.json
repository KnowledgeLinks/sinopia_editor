{
  "name": "sinopia_editor",
  "description": "Editor for BIBFRAME data forked from http://github.com/lcnetdev/bfe",
  "keywords": [
    "bibframe",
    "editor",
    "rdf"
  ],
  "version": "1.0.0",
  "homepage": "http://github.com/LD4P/sinopia_editor/",
  "repository": {
    "type": "git",
    "url": "https://github.com/LD4P/sinopia_editor.git"
  },
  "engines": {
    "node": ">= 11.0.0"
  },
  "contributors": [
    "Kirk Hess <khes@loc.gov>",
    "Jeremy Nelson",
    "Joshua Greben",
    "Naomi Dushay",
    "Sarav Shah",
    "Johnathan Martin",
    "Michael Giarlo",
    "Justin Coyne",
    "Peter Mangiafico",
    "Justin Littman",
    "Aaron Collier",
    "Huda Khan"
  ],
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "coveralls": "^3.0.6",
    "css-loader": "^2.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "~13.1.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-security": "^1.4.0",
    "expect-puppeteer": "^4.3.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-junit": "^6.4.0",
    "jest-localstorage-mock": "^2.4.0",
    "jest-prop-type-error": "^1.1.0",
    "jest-puppeteer": "^4.3.0",
    "jsdom": "15.0.0",
    "jsdom-global": "3.0.2",
    "puppeteer": "^1.19.0",
    "react-hot-loader": "^4.12.11",
    "redux-mock-store": "^1.5.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  },
  "bugs": {
    "url": "http://github.com/LD4P/sinopia_editor/issues"
  },
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "webpack -p --progress --mode production",
    "ci": "npm i && npm run eslint && npm run dev-build && npm run jest-cov",
    "dev-build": "webpack --progress --mode development",
    "dev-build-test": "npm run dev-build && npm run test",
    "dev-start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "eslint": "eslint --max-warnings 5 --color -c .eslintrc.js --ext js,jsx .",
    "eslint-fix": "eslint --max-warnings 5 --color -c .eslintrc.js --ext js,jsx --fix .",
    "eslint-fix-single": "eslint --color -c .eslintrc.js --fix",
    "jest-cov": "jest --coverage --colors --silent --runInBand",
    "jest-ci": "jest  --colors --silent --ci --runInBand --coverage --reporters=default --reporters=jest-junit && cat ./coverage/lcov.info | coveralls",
    "start": "npx babel-node server.js",
    "test": "jest --colors --silent --runInBand --detectOpenHandles",
    "test-verbose": "jest --colors --runInBand"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.21",
    "@fortawesome/free-solid-svg-icons": "^5.10.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "ajv": "^6.10.2",
    "amazon-cognito-identity-js": "^3.0.15",
    "babel-core": "^7.0.0-bridge.0",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^8.1.0",
    "event-stream": "^4.0.1",
    "express": "^4.17.1",
    "file-saver": "^2.0.2",
    "identity-obj-proxy": "^3.0.0",
    "lodash": "^4.17.15",
    "merge": "^1.2.1",
    "n3": "^1.2.0",
    "query-string": "^6.8.2",
    "rdf-ext": "^1.3.0",
    "react": "^16.9.0",
    "react-bootstrap": "^0.32.4",
    "react-bootstrap-table-next": "^3.1.8",
    "react-bootstrap-typeahead": "^3.4.6",
    "react-dom": "^16.9.0",
    "react-dropzone": "^7.0.1",
    "react-offcanvas": "^0.4.0",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "reselect": "^4.0.0",
    "shortid": "^2.2.14",
    "sinopia_server": "^3.0.0-beta5",
    "swagger-client": "^3.9.3",
    "x2js": "^3.3.1"
  },
  "jest": {
    "preset": "jest-puppeteer",
    "setupFilesAfterEnv": [
      "<rootDir>/setupEnzyme.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.js",
      "src/**/*.jsx",
      "!src/index.js"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "__tests__/fixtureLoaderHelper.js",
      "__tests__/integration/loginHelper",
      "__tests__/integration/previewRDFHelper",
      "__tests__/integration/integrationHelper"
    ],
    "setupFiles": [
      "jest-localstorage-mock",
      "jest-prop-type-error",
      "dotenv/config"
    ]
  }
}
