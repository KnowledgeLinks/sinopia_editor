{
  "name": "sinopia_editor",
  "description": "Editor for BIBFRAME data forked from http://github.com/lcnetdev/bfe",
  "keywords": [
    "bibframe",
    "editor",
    "rdf"
  ],
  "version": "0.0.1",
  "homepage": "http://github.com/LD4P/sinopia_editor/",
  "engines": {
    "node": "10.11.x"
  },
  "contributors": [
    "Kirk Hess <khes@loc.gov>",
    "Jeremy Nelson"
  ],
  "main": "Gruntfile.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/LD4P/sinopia_editor.git"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "coveralls": "^3.0.2",
    "es6-plato": "^1.0.18",
    "eslint": "^5.7.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-takeoff": "0.0.1",
    "file-loader": "^2.0.0",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-cssmin": "^3.0.0",
    "grunt-contrib-uglify-es": "^3.3.0",
    "grunt-eslint": "^21.0.0",
    "grunt-run": "^0.8.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-junit": "^5.1.0",
    "jest-puppeteer": "^3.4.0",
    "puppeteer": "^1.8.0",
    "react-hot-loader": "^4.3.11",
    "serve-static": "^1.13.2",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "bugs": {
    "url": "http://github.com/LD4P/sinopia_editor/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "grunt": "grunt",
    "eslint": "eslint --max-warnings 775 --color -c .eslintrc.js ./src",
    "test": "jest --colors",
    "jest-cov": "jest --coverage --colors",
    "jest-ci": "jest --ci --runInBand --coverage --reporters=default --reporters=jest-junit --colors  && cat ./coverage/lcov.info | coveralls",
    "analysis": "es6-plato -d static-analysis -n -e .eslintrc.js src src/lib static/js/config.js static/js/config-dev.js"
  },
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.3",
    "grunt": "^1.x.x",
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "jest": {
    "preset": "jest-puppeteer",
    "collectCoverageFrom": [
      "src/**/*.js",
      "static/js/config*.js"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "test-config.js"
    ]
  }
}
